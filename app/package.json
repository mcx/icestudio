{
  "name": "icestudio",
  "flavour": "develop",
  "version": "0.13.2w",
  "description": "Visual editor for open FPGA boards",
  "contributors": [
    {
      "name": "Carlos Venegas Arrabe",
      "url": "https://github.com/cavearr"
    },
    {
      "name": "Juan González Gómez",
      "url": "https://github.com/Obijuan"
    }
  ],
  "repository": "https://github.com/FPGAwars/icestudio",
  "updatecheck": "https://raw.githubusercontent.com/FPGAwars/icestudio/develop/app/package.json",
  "license": "GPL-2.0",
  "main": "index.html",
  "permissions": ["desktopCapture"],
  "chromium-args": "--enable-experimental-web-platform-features --enable-media-stream --enable-usermedia-screen-capturing --disable-backgrounding-occluded-window --allow-file-access-from-files --disable-web-security",
  "development": {
    "mode": false
  },
  "window": {
    "title": "iceStudio",
    "width": 900,
    "height": 620,
    "min_width": 800,
    "min_height": 300,
    "resizable": true,
    "position": "center",
    "icon": "resources/images/icestudio-logo.png",
     "resizable": true,
    "frame": true,
    "show": true
  },
  "apio": {
    "min": "0.9.5",
    "max": "1.0.0",
    "extras": [
      "blackiceprog",
      "tinyfpgab",
      "tinyprog",
      "icefunprog"
    ],
    "external": "",
    "branch": ""
  },
  "collection": "0.4.1",
  "engines": {},
  "node": ">= 21.1.0",
  "dependencies": {
    "ace-builds": "~1.37.5",
    "adm-zip": "0.4.16",
    "alertifyjs": "~1.13.1",
    "angular-gettext-tools": "2.3.5",
    "async": "~3.2.6",
    "backbone": "~1.6.0",
    "copy-paste": "~1.5.3",
    "extract-zip": "~1.6.5",
    "fast-copy": "^2.0.3",
    "fs-extra": "~1.0.0",
    "getos": "~3.0.1",
    "glob": "~11.0.1",
    "jspreadsheet-ce": "^4.15.0",
    "jquery": "~3.7.1",
    "jsuites": "^4.15.0",
    "lodash.debounce": "~3.1.1",
    "marked": "~0.3.19",
    "mustache": "^4.2.0",
    "node-lang-info": "~0.2.1",
    "openurl": "~1.1.1",
    "proper-lockfile": "^4.1.2",
    "rmdir": "~1.2.0",
    "rsyncwrapper": "~2.0.1",
    "select2": "~4.1.0-rc.0",
    "sha1": "~1.1.1",
    "shelljs": "^0.8.5",
    "spark-md5": "^3.0.1",
    "ssh-exec": "~2.0.0",
    "sudo-prompt": "~8.2.0",
    "svgo": "~0.7.2",
    "sweetalert2": "^11.15.10",
    "tmp": "0.0.33"
  },
  "readme": "../README.md",
  "keywords": [
    "FPGA",
    "open",
    "editor",
    "hardware"
  ]
}
